
<ul id="blog-<%= blog.id %>" class = "blogs">
	<li> 
	<span class="user" ><%= blog.user.email %></span> 
 	<span class="content"><%= blog.content %></span> 
  	<span  class="timestamp">Posted <%= time_ago_in_words(blog.created_at) %> ago.</span> 
  	<%= link_to "Edit", root_path %>
  	<%= link_to "Destroy", blog_path(blog.id), :method => :delete %>
  	</li>


</ul>